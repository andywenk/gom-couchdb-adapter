
test_storage:
  adapter: couchdb
  database: test
  delete_database_if_exists: true
  create_database_if_missing: true
  views:
    test_object_class_view:
      type: class
      class: Object
    test_map_reduce_view:
      type: map_reduce
      map:
        function(document) {
            if (document['number'] == 11) {
                emit(document['_id'], null);
            }
        }
